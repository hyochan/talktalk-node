{
  "name": "talktalk-node",
  "version": "0.0.0",
  "description": "TalkTalk Server built on top of Prisma and GraphQL Yoga",
  "repository": "https://github.com/dooboolab/talktalk-node",
  "private": true,
  "scripts": {
    "generate": "graphqlgen",
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/server.ts",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly src/server.ts",
    "format": "eslint --quiet src/**/*.ts",
    "format:fix": "eslint --fix --quiet src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "express-async-handler": "^1.1.4",
    "graphql": "14.3.0",
    "graphql-middleware": "^3.0.2",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.1.0",
    "prisma-client-lib": "^1.31.1"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.3",
    "@types/jest": "^24.0.12",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "graphqlgen": "^0.6.0-rc9",
    "jest": "^24.7.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "ts-node-dev": "^1.0.0-pre.32",
    "tsconfig-paths": "^3.8.0",
    "typescript": "^3.4.4"
  }
}
